- if user_signed_in?
  %ul
    - members_rating.each do |m|
      %li
        = image_tag m.photo, style: 'width: 50px; height: 50px'
        %span.member_name
          = m.name
        %span.member_rating
          = m.rating
        = link_to "Голосовать", vote_member_path(m), remote: true, method: :post, :"data-type" => :json, class: 'vote_link'